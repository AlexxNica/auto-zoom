{
  "manifest_version": 2,

  "key": "",

  "name": "Auto Zoom",
  "description": "This extension automatically zooms pages",
  "version": "0.1",

  "minimum_chrome_version": "43",

  "options_ui": {
    "page": "options_page.html",
    "chrome_style": true
  },

  "background": {
    "scripts": [
      "metrics/misc.js",
      "metrics/fontSize.js",
      "metrics/margin.js",
      "misc.js",
      "api.js",
      "persistent_set.js",
      "options.js",
      "background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
